<%=
  react_component 'AssetGroup', {
    title: asset_group.title.present? ? asset_group.title : 'Asset Group',
    thumb: (asset_group.thumb('360x360').url if asset_group.has_thumb?),
    type: asset_group.type,
    assets: asset_group.assets.map do |asset| {
      id: asset.id,
      name: asset.file.name,
      format: asset.file.format,
      selected: true
    } end,
    bundle_url: brand_guide_asset_bundles_url(subdomain: @brand_guide.slug)
  }, { prerender: true }
%>