<%=
  react_component 'AssetGroup', {
    title: asset_group.title,
    thumb: asset_group.try(:thumb, '108x108').try(:url),
    assets: asset_group.assets.map do |asset| {
      id: asset.id,
      name: asset.file.name,
      extension: asset.file.name.split('.').pop,
      selected: true
    } end,
    download_url: download_brand_guide_asset_group_path(@brand_guide, asset_group)
  }, { prerender: true }
%>